int qp, qm, qg;
String sqp,sqm,sqg;
int pp = 10, pm = 12, pg = 15;
int t;

sqp= JOptionPane.showInputDialog("Camisetas pequenas vendidas: ")
sqm= JOptionPane.showInputDialog("Camisetas m√©dias vendidas: ")
sqg= JOptionPane.showInputDialog("Camisetas grandes vendidas: ")

qp= Integer.parseInt(sqp);
qm = Integer.parseInt(sqm);
qg = Integer.parseInt(sqg);

t = qp * pp + qm * pm + qg * pg;

JOptionPane.showMessageDialog(null, "Total: R$ " + t);
