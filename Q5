package exercicio_fix;

import javax.swing.JOptionPane;
/**
 *
 * @author Discente
 */
public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

    
    double r,l;
        
        String sr;
   
        
        sr=JOptionPane.showInputDialog(null, "Valor em real: ");
       
       
        
        r = Double.parseDouble(sr);
       
        l= r/5;
     
       
      JOptionPane.showMessageDialog(null, l+" litros (5R$ um litro)");
       
    }
}
