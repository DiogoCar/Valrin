    double b,h,ar;
        
        String bs,hlt;
   
        
        bs=JOptionPane.showInputDialog(null, "Digite a base: ");
        hlt=JOptionPane.showInputDialog(null, "Digite a altura: ");
       
        
        b = Double.parseDouble(bs);
        h = Double.parseDouble(hlt);
        
      ar= b*h;
       
      JOptionPane.showMessageDialog(null, "A area do quadrado Ã©: " + ar);
       
